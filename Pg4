class kolonko{
    public static void main(String Args[]){
        int fnum=0;
        int snum=1;
        System.out.println("1");
        for(int i=2;i<=4;i++){
            for(int j=2;j<=i;j++){
                int fib=fnum+snum;
                System.out.print(fib+" ");
                fnum=snum;
                snum=fib;
            }
            System.out.println();
        }
    }
}
