class dead{
    public static void main(String Args[]){
        int f=1;
        for(int i=1;i<=4;i++){
            for(int j=1;j<=i;j++){
              if(f%2!=0){
                  System.out.print(f+" ");
              }
              f=f+2;
            }
            System.out.println();
        }
    }
}
