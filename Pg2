class mrittu{
    public static void main(String Args[]){
        for(int i=1;i<=5;i++){
            if(i==1 || i==5){
                for(int j=1;j<=5;j++){
                    System.out.print("*");
                }
            }else{
                System.out.print("*");
                for(int j=2;j<=4;j++){
                    System.out.print(" ");
                }
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
